{
  "name": "akfun-vue-template",
  "version": "1.0.1",
  "description": "akfun前端脚手架/Vue类型项目模板",
  "author": "ldan@wibetter",
  "license": "MIT",
  "scripts": {
    "dev": "akfun dev --env=local",
    "build": "akfun build --env=online",
    "format": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+http://git@github.com:wibetter/akfun-vue-template.git"
  },
  "bugs": {
    "url": "https://github.com/wibetter/akfun-vue-template/issues"
  },
  "dependencies": {
    "@better-scroll/core": "^2.0.0-beta.4",
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "jsonp": "^0.2.1",
    "md5": "^2.2.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "akfun": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ]
}
